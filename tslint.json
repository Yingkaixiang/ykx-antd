{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "member-access": false,
    "no-console": [false],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "no-any": false,
    "no-empty-interface": false,
    "jsx-boolean-value": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "ignore"
        }
      }
    ]
  }
}
